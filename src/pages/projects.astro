---
import MainLayout from '@/layouts/MainLayout';
import octokit from '@/octokit';
const projectsData = await octokit.rest.repos.listForUser({ username: "redanthrax" });
const projects = projectsData.data.map(project => ({
    ...project,
}));
console.log(projects[0]);
---

<MainLayout>
    <h2 class="mb-6 title">Projects</h2>
    {
        projects.map((project) => 
            <h2 class="text-lg">{project.name}</h2>
        )
    }
</MainLayout>
